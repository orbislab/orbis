{
    "80": {
        "localhost": {
            "activation": true,
            "description": "Develop Core Engine",
            "dir": "$ORBIS_HOME/web-src",
            "session-expire": "1w",
            "doc-index": ["index.html"],
            "filter": {
                "api": [".api.js"],
                "apidoc": ["/api.doc"],
                "orbis": [".html"],
                "php": [".php"],
                "central-repo": ["/central"]
            }
        }
    },
    "443": {
        "ssl": {
            "key": "$ORBIS_HOME/sslcert/key.pem",
            "cert": "$ORBIS_HOME/sslcert/server.crt"
        },
        "default": {
            "description": "Develop Core Engine",
            "dir": "$ORBIS_HOME/web-src",
            "session-expire": "1w",
            "doc-index": ["index.html"],
            "filter": {
                "api": [".api.js"],
                "apidoc": ["/api.doc"],
                "orbis": [".html"],
                "php": [".php"],
                "central-repo": ["/central"]
            }
        },
        "127.0.0.1": {
            "activation": true,
            "description": "Develop Core Engine",
            "dir": "$ORBIS_HOME/test",
            "hidden-path": [
                "/template",
                "/error"
            ],
            "error-page": {
                "404": "error/404.html",
                "500": "error/500.html"
            },
            "doc-index": ["index.html"],
            "filter": {
                "api": [".api.js"],
                "apidoc": ["/api.doc"],
                "orbis": [".html"],
                "php": [".php"],
                "central-repo": ["/central"]
            }
        }
    }
}